            <exclude wildcard="ds316.[123456789]*" atomic="true" collection="true"/>
            <exclude wildcard="ds5[123456789]*" atomic="true" collection="true"/>
            <exclude wildcard="ds50[013456789]*" atomic="true" collection="true"/>
            <exclude wildcard="ds502.[123456789]*" atomic="true" collection="true"/>
            <exclude wildcard="ds6[01456789]*" atomic="true" collection="true"/>
            <exclude wildcard="ds62[012345678]*" atomic="true" collection="true"/>
