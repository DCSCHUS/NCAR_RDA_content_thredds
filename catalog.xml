            <serviceName>all</serviceName>
            <!--publisher>
                <name vocabulary="DIF">USGS/ER/WHCMSC/Dr. Richard P. Signell</name>
                <contact url="http://www.usgs.gov/" email="rsignell@usgs.gov"/>
            </publisher-->
        </metadata>
        <filter>
            <include wildcard="ds083.2" atomic="false" collection="false"/>
            <include wildcard="ds093.1" atomic="false" collection="false"/>
            <include wildcard="ds094.1" atomic="false" collection="false"/>
            <include wildcard="ds277.6" atomic="false" collection="false"/>
            <include wildcard="ds285.3" atomic="false" collection="false"/>
            <include wildcard="ds629.1" atomic="false" collection="false"/>
            <include wildcard="ds629.2" atomic="false" collection="false"/>
            <include wildcard="ds629.3" atomic="false" collection="false"/>
            <include wildcard="ds629.5" atomic="false" collection="false"/>
            <include wildcard="ds629.6" atomic="false" collection="false"/>
            <include wildcard="ds631.0" atomic="false" collection="false"/>
            <exclude wildcard="ds[14789]*" atomic="true" collection="true"/>
            <exclude wildcard="ds0[1234567]*" atomic="true" collection="true"/>
            <exclude wildcard="ds08[012456789]*" atomic="true" collection="true"/>
